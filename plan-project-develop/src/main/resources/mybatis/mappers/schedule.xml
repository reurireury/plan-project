	<resultMap id="scheResult" type="com.spring.market_1.schedule.dto.ScheduleDTO">
		<result property="id" column="id"/>
		<result property="title" column="title"/>
		<result property="content" column="content"/>
		<result property="start1" column="start1"/>
		<result property="end1" column="end1"/>
	</resultMap>
	
	<select id="scheduleList" resultMap="scheResult">
		<![CDATA[
			select *  from schedule
		]]>
	</select>
	
	<insert id="AddSchedule" parameterType="com.spring.market_1.schedule.dto.ScheduleDTO">
	    <![CDATA[
	        INSERT INTO schedule (title,start1,end1,content) 
	        VALUES
        ]]>
          (#{title},#{start1},#{end1},#{content})
	   
	</insert>
	
	

	
	
	
	
	
	
	
